{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  {
    "HealthChecksUI": {
      "HealthChecks": [
        {
          "Name": "Event_Management_System_API",
          "Uri": "/health"
        },
        {
          "Name": "CustomHealthCheck",
          "Uri": "/health/custom"
        }
      ],
      "EvaluationTimeInSeconds": 5
    }
  }
}
